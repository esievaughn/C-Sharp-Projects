@using CarInsurance.Models
@model List<Insuree>
  
<div class="row no-gutters m-3">
  <div class="card col-sm-12 col-md-10 col-lg-6 mx-auto shadow">
    <div class="card-header text-center">
      <h3 class="m-0">All Quotes Issued</h3>
    </div>
    <div class="card-body">
      @foreach (var insuree in Model)
       {
      <div class="row mb-3">
        <div class="col">
          @insuree.FirstName
        </div>
        <div class="col">
          @insuree.LastName
        </div>
        <div class="col">
          @insuree.EmailAddress
        </div>
        <div class="col">
          @insuree.Quote
        </div>
        <br />
        <br />
      </div>
       }
    </div>
  </div>
</div>
